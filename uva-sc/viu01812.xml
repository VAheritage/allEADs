      <unittitle>Austin Kennell Papers 
         <unitdate type="inclusive" label="Date" era="ce" calendar="gregorian">
         1968-1999</unitdate></unittitle>
      <unitid label="Accession number">10553-ak</unitid>
      <physdesc label="Physical Characteristics">ca. 2,000 items (6
         Hollinger boxes, ca. 2.5 linear feet)</physdesc>
