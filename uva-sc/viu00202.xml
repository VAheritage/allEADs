            <physdesc><genreform>ALS</genreform>, 
                  <extent>1 p.</extent></physdesc>
          </did>
          <scopecontent>
            <p>[Expresses pleasure of manuscript acceptance;
                  mentions ill health.]</p>
          </scopecontent>
        </c02>
        <c02 level="item" id="d1e314">
          <did>
            <unittitle><persname>Rose Terry Cooke</persname>to 
