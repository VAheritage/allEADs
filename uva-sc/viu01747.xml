      <repository>Special Collections, University of Virginia
         Library</repository>
      <unittitle>World War I Papers and Memorabilia Chiefly
         Regarding Thomas R. Pleucker 
         <unitdate type="inclusive" label="Date" era="ce" calendar="gregorian">
         1914-1930</unitdate></unittitle>
      <unitid label="Accession number">10875-r</unitid>
