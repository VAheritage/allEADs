         <repository>Special Collections, Earl Gregg Swem Library,
         College of William and Mary</repository>
         <unittitle label="Title">Burwell Papers, 
         <unitdate type="inclusive" label="Date">
         1853-1911.</unitdate></unittitle>
         <physdesc label="Extent">230 pieces.</physdesc>
         <langmaterial label="Language">
            <language langcode="eng">English</language>
         </langmaterial>
         <abstract label="Abstract">Papers of the Burwell
         Family.</abstract>
         <origination label="Creator">
            <famname>Burwell Family.</famname>
