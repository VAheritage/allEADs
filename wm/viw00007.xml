      <unittitle label="Title">Smith-Walker Papers, 
         <unitdate type="inclusive" label="Date" era="ce" calendar="gregorian">
         1764-1916.</unitdate></unittitle>
      <unitid label="Collection number">Mss. 39.1 Sm8</unitid>
